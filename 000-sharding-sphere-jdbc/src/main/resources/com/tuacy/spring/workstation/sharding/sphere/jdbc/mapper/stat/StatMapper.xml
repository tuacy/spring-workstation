<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tuacy.spring.workstation.sharding.sphere.jdbc.mapper.stat.StatMapper">

    <!-- 插入数据 -->
    <insert id="insertItem">
        INSERT INTO `stat_acc` (`pointPkId`, `recTime`, `value`, `incrValue`) VALUES (#{item.pointPkId}, #{item.recTime}, #{item.value}, #{item.incrValue});
    </insert>

</mapper>
